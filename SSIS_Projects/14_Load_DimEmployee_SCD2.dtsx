          SQLTask:Connection="{D4151C45-30C2-48E4-8040-D073AC16723D}"
          SQLTask:SqlStatementSource="USE HR_DW_Variant7;&#xA;GO&#xA;&#xA;DECLARE @Now DATETIME2(0) = SYSDATETIME();&#xA;&#xA;-- 1) Закрити поточні записи, якщо атрибути змінилися&#xA;UPDATE d&#xA;SET&#xA;  d.EffectiveToDT = DATEADD(second, -1, @Now),&#xA;  d.IsCurrent = 0&#xA;FROM dbo.DimEmployee d&#xA;JOIN dbo.StgEmployee s&#xA;  ON s.EmployeeID_NK = d.EmployeeID_NK&#xA;WHERE d.IsCurrent = 1&#xA;  AND (&#xA;       d.EmployeeCode &lt;&gt; s.EmployeeCode&#xA;    OR d.FullName     &lt;&gt; s.FullName&#xA;    OR ISNULL(d.Gender,'') &lt;&gt; ISNULL(s.Gender,'')&#xA;    OR ISNULL(d.BirthDate,'1900-01-01') &lt;&gt; ISNULL(s.BirthDate,'1900-01-01')&#xA;    OR d.HireDate &lt;&gt; s.HireDate&#xA;    OR ISNULL(d.TerminationDate,'1900-01-01') &lt;&gt; ISNULL(s.TerminationDate,'1900-01-01')&#xA;    OR d.Status &lt;&gt; s.Status&#xA;  );&#xA;&#xA;-- 2) Вставити нових або нову &quot;версію&quot; змінених&#xA;INSERT dbo.DimEmployee&#xA;(&#xA;  EmployeeID_NK, EmployeeCode, FullName, Gender, BirthDate, HireDate, TerminationDate, Status,&#xA;  EffectiveFromDT, EffectiveToDT, IsCurrent&#xA;)&#xA;SELECT&#xA;  s.EmployeeID_NK, s.EmployeeCode, s.FullName, s.Gender, s.BirthDate, s.HireDate, s.TerminationDate, s.Status,&#xA;  @Now AS EffectiveFromDT,&#xA;  CONVERT(datetime2(0),'9999-12-31 23:59:59') AS EffectiveToDT,&#xA;  1 AS IsCurrent&#xA;FROM dbo.StgEmployee s&#xA;LEFT JOIN dbo.DimEmployee d&#xA;  ON d.EmployeeID_NK = s.EmployeeID_NK&#xA; AND d.IsCurrent = 1&#xA;WHERE d.EmployeeID_NK IS NULL&#xA;   OR (&#xA;       d.EmployeeCode &lt;&gt; s.EmployeeCode&#xA;    OR d.FullName     &lt;&gt; s.FullName&#xA;    OR ISNULL(d.Gender,'') &lt;&gt; ISNULL(s.Gender,'')&#xA;    OR ISNULL(d.BirthDate,'1900-01-01') &lt;&gt; ISNULL(s.BirthDate,'1900-01-01')&#xA;    OR d.HireDate &lt;&gt; s.HireDate&#xA;    OR ISNULL(d.TerminationDate,'1900-01-01') &lt;&gt; ISNULL(s.TerminationDate,'1900-01-01')&#xA;    OR d.Status &lt;&gt; s.Status&#xA;   );" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
