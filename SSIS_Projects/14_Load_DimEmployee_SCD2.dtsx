          SQLTask:SqlStatementSource="USE HR_DW_Variant7;&#xA;GO&#xA;&#xA;IF OBJECT_ID('dbo.StgEmployee', 'U') IS NULL&#xA;BEGIN&#xA;  CREATE TABLE dbo.StgEmployee (&#xA;    EmployeeID_NK     INT            NOT NULL,&#xA;    EmployeeCode      NVARCHAR(20)    NOT NULL,&#xA;    FullName          NVARCHAR(160)  NOT NULL,&#xA;    Gender            CHAR(1)        NULL,&#xA;    BirthDate         DATE           NULL,&#xA;    HireDate          DATE           NOT NULL,&#xA;    TerminationDate   DATE           NULL,&#xA;    Status            VARCHAR(20)    NOT NULL,&#xA;    LoadDT            DATETIME2(0)   NOT NULL DEFAULT SYSDATETIME(),&#xA;    CONSTRAINT PK_StgEmployee PRIMARY KEY (EmployeeID_NK)&#xA;  );&#xA;END;&#xA;&#xA;TRUNCATE TABLE dbo.StgEmployee;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
